{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/styles/(.*)",
      "destination": "/public/styles/$1"
    },
    {
      "source": "/script/(.*)",j
      "destination": "/public/script/$1"
    },
    {
      "source": "/img/(.*)",
      "destination": "/public/img/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}
